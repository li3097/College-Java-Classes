///<Pair> Use generic class to print pair
//CSIS312 - <Assignment 7>
//Citations if necessary>--
package pair;

/**
 *
 * @author lirby
 */
public class Pair <F,S>{

    //variables
    private F first;
    private S second;
    
    //constructor
    public Pair(F first, S second){
        this.first 
        = first;
        this.second = second;
    }
    
    //setters
    public void setFirst(F first) {
        this.first = first;
    
    }
    public void setSecond(S second){
        this.second = second;
    }
    
    //getters
    public F getFirst(){
        return first;
    }  
    public S getSecond(){
        return second;
    }
}//end class

/*
